<h2>Total Results: <%= facade.total_results %></h2>

<h2>15 Closest Stations</h2>

<% facade.stations.each do |station| %>
  <ul class='station'>
    <li class='name'><%= station.name %></li>
    <li class='address'><%= station.address %></li>
    <li class='fuel_types'><%= station.fuel_types %></li>
    <li class='distance'><%= station.distance %></li>
    <li class='access_times'><%= station.access_times %></li>
  </ul>
<% end %>
